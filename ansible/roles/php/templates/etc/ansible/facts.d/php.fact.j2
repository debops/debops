{% set php__tpl_sury = (ansible_local.php.sury if (ansible_local|d() and ansible_local.php|d() and ansible_local.php.sury|d()) else php__sury) | bool %}
{% if php__sury | bool %}
{%   set php__tpl_sury = php__sury | bool %}
{% endif %}
{
"enabled": "true",
"long_version": "{{ php__long_version }}",
