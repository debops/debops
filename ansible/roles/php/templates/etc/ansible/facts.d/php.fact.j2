"version": "{{ php__version }}",
"etc_base": "{{ php__etc_base }}",
"lib_base": "{{ php__lib_base }}",
"run_base": "{{ php__run_base }}",
"logrotate_lib_base": "{{ php__logrotate_lib_base }}"
}
